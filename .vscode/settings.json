{
  "latex-workshop.latex.toolchain": [
    {
      "command": "xelatex",
      "args": [
        "--output-directory=out",
        "--interaction=nonstopmode",
        "%DOC%.tex"
      ]
    },
    {
      "command": "xelatex",
      "args": [
        "--output-directory=out",
        "--interaction=nonstopmode",
        "%DOC%.tex"
      ]
    }
  ],
  "latex-workshop.latex.clean.enabled": false,
  "latex-workshop.latex.outputDir": "out",
  "latex-workshop.latex.autoBuild.onSave.enabled": true,
  "latex-workshop.latex.autoBuild.onTexChange.enabled": true,
  "spellright.groupDictionaries": false,
  "spellright.language": "en-GB",
  "latex-workshop.debug.showUpdateMessage": false,
  "files.exclude": {
    "lib": true,
    ".shards": true,
    "**/.git": true,
    "**/.gitkeep": true,
    "**/out": true,
    "**/pyg": false
  },
  "editor.wordWrap": "bounded",
  "material-icon-theme.hidesExplorerArrows": true,
  "material-icon-theme.folders.associations": {
    "report": "docs",
    "research": "docs",
    "implementation": "docs",
    "pyg": "temp",
    "bin": "config",
    "hibari": "components",
    "json_api": "components",
    "kitsu": "components"
  }
}
